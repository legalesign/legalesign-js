{
  "name": "legalesign-js",
  "version": "0.0.4",
  "type": "module",
  "main": "./dist/vite-vanilla-ts-lib-starter.cjs",
  "module": "./dist/vite-vanilla-ts-lib-starter.mjs",
  "exports": {
    ".": {
      "require": "./dist/vite-vanilla-ts-lib-starter.cjs",
      "import": "./dist/vite-vanilla-ts-lib-starter.mjs"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite --host",
    "test": "glob -c \"tsx --env-file-if-exists .env --env-file-if-exists .env.test --test --test-reporter spec \" \"./test/**/*.test.ts\"",
    "build": "tsc"
  },
  "devDependencies": {
    "@babel/core": "^7.25.9",
    "@babel/preset-env": "^7.25.9",
    "@babel/preset-typescript": "^7.25.9",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "babel-jest": "^29.7.0",
    "glob": "^11.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "^5.7.0-dev.20241024"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.620.0",
    "amazon-cognito-identity-js": "^6.3.12",
    "axios": "^1.7.2",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.0"
  },
  "publishConfig": {
    "@legalesign:registry": "https://npm.pkg.github.com"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/setEnvVars.js"
    ]
  }
}
